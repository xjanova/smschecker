{
    "name": "thaiprompt/smschecker-laravel",
    "description": "SMS Payment Checker - Laravel Payment Gateway Plugin for Thai Bank SMS auto-matching",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Thaiprompt",
            "homepage": "https://github.com/xjanova"
        }
    ],
    "require": {
        "php": "^8.1",
        "laravel/framework": "^10.0|^11.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0|^11.0",
        "orchestra/testbench": "^8.0|^9.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit --testdox"
    },
    "minimum-stability": "stable"
}
